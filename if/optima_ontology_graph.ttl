        "a été la technique mise en œuvre dans"@fr,
        "foi técnica da"@pt,
        "был техникой для"@ru ;
    rdfs:domain crm:E55_Type ;
    rdfs:range crm:E7_Activity ;
    rdfs:subPropertyOf crm:P125i_was_type_of_object_used_in .

